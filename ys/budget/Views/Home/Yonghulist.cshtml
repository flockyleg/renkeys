        a = parseInt(a) + parseInt(intstep);
        //if (parseInt(a1) > parseInt(a2)) {
        //    a1 = a2;
        //}
        //if (parseInt(a) < parseInt(a1)) {
        //    a = a1;
        //}
        //if (parseInt(a) > parseInt(a2)) {
        //    a = a2;
        //}


        $("#txtdangqianye").val(a);

        var s = "../Home/Yonghulist?p=" + a;
        window.location = s;
    }

    function comboitemclick(txtid, txtvalue) {
        var a = document.getElementById(txtid)
        a.value = txtvalue;
    }

    function shanchu(id) {
        var b1 = confirm("确认要删除该用户？");
        if (b1) {
            $.ajax({
                type: 'POST',
                url: "/Home/Yonghushanchu",
                data: {
                    id: id
                },
                dataType: "json",
                success: function (sesponseTest) {
                    switch (sesponseTest) {
                        case 900001:
                            alert("系统错误，请稍后重试。");
                            break;
                        case 900003:
                            alert("删除失败。");
                            break;
                        default:
                            alert("删除成功，点刷新按钮刷新列表。");
                            break;
                    }
                }
            });
        }
    }


    function configDiv_Yd(div_id) {
        var id = $("#txtyhid").val();
        if (id == "" || undefined || null) {
            id = "";
        }
        var xh = $("#txtxh").val();
        if (xh == "" || undefined || null) {
            $("#lblMsg1").val('请输入排序号。');
            return false;
        }
        var dlm = $("#txtdlm").val();
        if (dlm == "" || undefined || null) {
            $("#lblMsg1").val('请输入登录名。');
            return false;
        }
        var xm = $("#txtxm").val();
        if (xm == "" || undefined || null) {
            $("#lblMsg1").val('请输入姓名。');
            return false;
        }
        var mm = $("#txtmm").val();
        if (mm == "" || undefined || null) {
            mm = "";
        }
        var bm = $("#txtbm").val();
        if (bm == "" || undefined || null) {
            bm = "";
        }
        var js = $("#txtjs").val();
        if (js == "" || undefined || null) {
            js = "";
        }
        var chkObjs = "";
        var obj = document.getElementsByName("chkqx")
        for (var i = 0; i < obj.length; i++) { //遍历Radio 
            if (obj[i].checked) {
                chkObjs = chkObjs + " " + obj[i].value;
            }
        }
        //alert(chkObjs);
        var bz = $("#txtbz").val();
        if (bz == "" || undefined || null) {
            bz = "";
        }

        $.ajax({
            type: 'POST',
            url: "/Home/SaveYonghu",
            data: {
                id: id,
                xh: xh,
                dlm: dlm,
                xm: xm,
                mm: mm,
                qx: chkObjs,
                bz: bz,
                bm: bm,
                js: js
            },
            dataType: "text",
            success: function (sesponseTest) {
                switch (sesponseTest) {
                    case 401:
                        alert('保存失败，未知的用户。');
                        break;
                    case 402:
                        alert('保存失败，登录名或姓名未输入。');
                        break;
                    default:
                        hideDiv(div_id);
                        alert('保存成功，点击刷新按钮获取更新列表。');
                        break;
                }
            }
        });
    }

    function comboitemclick(txtid, txtvalue) {
        var a = document.getElementById(txtid)
        a.value = txtvalue;
    }

    document.getElementById("worktable").style.height = document.body.clientHeight + "px";
</script>



