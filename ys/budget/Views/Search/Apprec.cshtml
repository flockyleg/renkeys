@{
    ViewBag.Title = "申办记录";
}

<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <style>
        p.xingmingshengri {
            font-size: 15px;
            font-family: 微软雅黑, Microsoft YaHei, Microsoft YaHei UI, 黑体, Arial, Helvetica, sans-serif;
            color: black;
        }
    </style>

</head>

<body style="background-color: #2A293B;">
    <table style="width: 100%; background-color: white; min-height: 400px;">
        <tr>
            <td style="height:100%;vertical-align:top;">
                <div class="divbackground">

                    <li class="list-group-item">
                        <br>
                        <ol class="breadcrumb">
                            <p class="xingmingshengri">查 询 结 果</p>
                        </ol>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">受理单号</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="2018000001">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">申办时间</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value=@DateTime.Now.ToString("yyyy-mm-dd HH:mm")>
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">使用国家</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="美国">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">签证类型</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="旅游签证">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">申请人员</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="张三丰、章四风">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">材料递送</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="直接上传">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">短信手机</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="13809090909">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">备用电话</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="13345678901">
                        </div>
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">备　　注</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="">
                        </div>
                    </li>
                    <li class="list-group-item">
                        <div class="input-group wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <span class="input-group-addon">办理情况</span>
                            <input type="text" class="form-control" readonly="readonly" aria-label="..." value="" style="background-color:white;">
                        </div>
                    </li>

                    <li class="list-group-item">
                        <table style="width:100%;height:60px;">
                            <tr style="width:100%">
                                <td style="width: 100px; text-align: center; "><button class="btn btn-primary" type="button" onclick="fanhuisy();">&nbsp;&nbsp;返回首页&nbsp;&nbsp;</button></td>
                                <td style="text-align: center; "></td>
                                <td style="width: 100px; text-align: center; "><button class="btn btn-primary" type="button" onclick="cxcx();">&nbsp;&nbsp;重新查询&nbsp;&nbsp;</button></td>
                            </tr>
                        </table>
                    </li>
                </div>
            </td>
        </tr>
    </table>

</body>
</html>

<script type="text/javascript" language="javascript">
    function fanhuisy() {
        window.location = "/Home/Index";
    }
    function cxcx() {
        window.location = "Index";
    }
</script>



