@model applyvisa.Models.cls实际盈亏汇总表s

@{

    Layout = null;
    ViewBag.Title = "实际盈亏汇总表";

    int i = 0;
    decimal dec1 = 0.00m;
    decimal dec2 = 0.00m;
    decimal dec3 = 0.00m;
    decimal dec4 = 0.00m;
    decimal dec5 = 0.00m;
    decimal dec6 = 0.00m;
    decimal dec7 = 0.00m;
    decimal dec8 = 0.00m;
}


<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <script type="text/javascript" src="~/Others/js/alert.js"></script>

    <style>
        .parent {
            width: 100%;
            height: 230px;
            margin: 0 auto;
            text-align: center;
        }

        .button {
            font-size: 16px;
            font-family: 微软雅黑, Microsoft YaHei, Microsoft YaHei UI, 黑体, Arial, Helvetica, sans-serif;
            font-stretch: expanded;
            color: #333333;
        }
    </style>

</head>

<body style="background-color: #2A293B;">

    <input id="wheresql_rpt2_gsm" type="hidden" value="@ViewBag.wheresql_rpt2_gsm">
    <input id="wheresql_rpt2_rq1" type="hidden" value="@ViewBag.wheresql_rpt2_rq1">
    <input id="wheresql_rpt2_rq2" type="hidden" value="@ViewBag.wheresql_rpt2_rq2">

    <table id="worktable" style="width: 1024px; background-color: white; min-height: 800px; margin-right: auto; margin-left: auto;">
        <tr style="border-top:solid 1px #ede9e9;background-color:#f1eded;">
            <td style="text-align: center; vertical-align: middle;height:40px;">
                <p style="font-size:18px;">----- 汇 总 表 -----</p>
            </td>
        </tr>
        <tr>
            <td style="text-align: right; vertical-align: middle;height:40px;">
                <button class="btn btn-sm" type="button" style="width:100px;" onclick="Export2Excel_rpt2a();">导出</button>
            </td>
        </tr>
        <tr>
            <td style="height:100%;vertical-align:top;" colspan="8">
                <div class="divbackground">
                    <li class="list-group-item">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>公司名称</th>
                                    <th>项目实际收入(A)</th>
                                    <th>销项税额(B)</th>
                                    <th>项目实际支出(C)</th>
                                    <th>进项税额(D)</th>
                                    <th>应交税额(E=B-D)</th>
                                    <th>GP(F=A-C-E)</th>
                                    <th>固定费用支出(G)</th>
                                    <th>盈亏(H=F-G)</th>
                                    <th>操作</th>
                                </tr>
                            </thead>
                            <tbody>

                                @{
                                    dec1 = 0.00m;
                                    dec2 = 0.00m;
                                    dec3 = 0.00m;
                                    dec4 = 0.00m;
                                    dec5 = 0.00m;
                                    dec6 = 0.00m;
                                    dec7 = 0.00m;
                                    dec8 = 0.00m;
                                    for (i = 0; i < Model.记录集.Count; i++)
                                    {
                                        dec1 = dec1 + Model.记录集[i].项目实际收入;
                                        dec2 = dec2 + Model.记录集[i].销项税额;
                                        dec3 = dec3 + Model.记录集[i].项目实际支出;
                                        dec4 = dec4 + Model.记录集[i].进项税额;
                                        dec5 = dec5 + Model.记录集[i].应交税额;
                                        dec6 = dec6 + Model.记录集[i].GP;
                                        dec7 = dec7 + Model.记录集[i].固定费用支出;
                                        dec8 = dec8 + Model.记录集[i].盈亏;
                                        <tr>
                                            <td>@Model.记录集[i].公司名称</td>
                                            <td>@Model.记录集[i].项目实际收入</td>
                                            <td>@Model.记录集[i].销项税额</td>
                                            <td>@Model.记录集[i].项目实际支出</td>
                                            <td>@Model.记录集[i].进项税额</td>
                                            <td>@Model.记录集[i].应交税额</td>
                                            <td>@Model.记录集[i].GP</td>
                                            <td>@Model.记录集[i].固定费用支出</td>
                                            <td>@Model.记录集[i].盈亏</td>
                                            <td><a href="javascript:showDetail('@Model.记录集[i].公司名称','@ViewBag.wheresql_rpt2_rq1','@ViewBag.wheresql_rpt2_rq2');">明细</a></td>
                                        </tr>
                                    }

                                    <tr>
                                        <td>合计</td>
                                        <td>@dec1</td>
                                        <td>@dec2</td>
                                        <td>@dec3</td>
                                        <td>@dec4</td>
                                        <td>@dec5</td>
                                        <td>@dec6</td>
                                        <td>@dec7</td>
                                        <td>@dec8</td>
                                        <td></td>
                                    </tr>
                                }

                            </tbody>
                        </table>

                        <br>

                        <table style="width:100%;height:60px;">
                            <tr style="width:100%">
                                <td></td>
                                <td style="text-align: center; width:360px;">

                                    <div class="input-group">
                                        <span class="input-group-btn">
                                            <button id="buttoexcel" class="btn btn-default" type="button" onclick="Export2Excel_rpt2a();">共 @Model.记录集.Count 条，导出</button>
                                        </span>
                                    </div>

                                </td>
                                <td></td>
                            </tr>
                        </table>

                    </li>



                </div>
            </td>
        </tr>

        <tr>
            <td style="height:100%;vertical-align:top;" colspan="8">
                <div id="rptb">

                </div>
            </td>
        </tr>
        <tr>
            <td style="height:100%;vertical-align:top;" colspan="8">
                <div id="rptc">

                </div>
            </td>
        </tr>

    </table>

</body>
</html>



<script type="text/javascript">

    function showDetail(gsm, rq1, rq2) {
        $("#rptb").load("/Report/rpt2b", { "gsm": gsm, "rq1": rq1, "rq2": rq2 });
        $("#rptc").load("/Report/rpt2c", { "gsm": gsm, "rq1": rq1, "rq2": rq2 });
    }



    function Export2Excel_rpt2a() {
        var gsm = $("#wheresql_rpt2_gsm").val();
        if (gsm == "" || undefined || null) {
            gsm = "";
        }
        var rq1 = $("#wheresql_rpt2_rq1").val();
        if (rq1 == "" || undefined || null) {
            rq1 = "";
        }
        var rq2 = $("#wheresql_rpt2_rq2").val();
        if (rq2 == "" || undefined || null) {
            rq2 = "";
        }

        gsm = escape(gsm);
        rq1 = escape(rq1);
        rq2 = escape(rq2);
        var s = "../Report/Export2Excel_rpt2a?gsm=" + gsm + "&rq1=" + rq1 + "&rq2=" + rq2;
        window.location = s;
    }

</script>



