@using applyvisa.Models;

@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "中证通订单应收表";
    
    int i = 0;
}


<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <script type="text/javascript" src="~/Others/js/alert.js"></script>

    <style>
        .parent {
            width: 100%;
            height: 230px;
            margin: 0 auto;
            text-align: center;
        }

        .button {
            font-size: 16px;
            font-family: 微软雅黑, Microsoft YaHei, Microsoft YaHei UI, 黑体, Arial, Helvetica, sans-serif;
            font-stretch: expanded;
            color: #333333;
        }
    </style>

</head>

<body style="background-color: #2A293B;">
    <table id="worktable" style="width: 1024px; background-color: white; height: 60px; margin-right: auto; margin-left: auto;">
        <tr>
            <td style="text-align: center; vertical-align: middle;height:80px;">
                <p style="font-size:18px;">中证通订单应收表</p>
            </td>
        </tr>
        <tr>
            <td>
                <table id="worktable" style="width: 1024px; background-color: white; height: 50px;">
                    <tr>
                        <td style="width:70px;text-align:center;font-size:13px;">
                            &nbsp;&nbsp; 子订单号
                        </td>
                        <td style="width:250px;">
                            <input id="txtzddh" type="text" class="form-control" style="width: 250px;">
                        </td>
                        <td style="width:70px;text-align:center;font-size:13px;">
                            &nbsp;&nbsp; 当事人
                        </td>
                        <td style="width: 250px;">
                            <input id="txtdsr" type="text" class="form-control" style="width: 250px;">
                        </td>
                        <td style="width:80px;text-align:center;font-size:13px;">
                            &nbsp;&nbsp; 承办公证处
                        </td>
                        <td style="width: 250px;">
                            <input id="txtgzc" type="text" class="form-control" style="width: 250px;">
                        </td>
                    </tr>
                </table>
                <table id="worktable" style="width: 1024px; background-color: white; height: 50px;">
                    <tr>
                        <td style="width:70px;text-align:center;font-size:13px;">
                            &nbsp;&nbsp;申请日期
                        </td>
                        <td style="width:170px;">
                            <input id="txtrq1" type="date" class="form-control" style="width: 170px;">
                        </td>
                        <td style="width:5px;text-align:center;font-size:13px;">
                            ～
                        </td>
                        <td style="width:170px;">
                            <input id="txtrq2" type="date" class="form-control" style="width: 170px;">
                        </td>
                        <td style="width: 540px;"></td>
                    </tr>
                </table>
                <table id="worktable" style="width: 1024px; background-color: white; height: 50px;">
                    <tr>
                        <td style="width:auto;">
                            <input id="lblMsg1" type="text" readonly="readonly" style="border-style:none;color:red;">
                        </td>
                        <td style="width:120px;">
                            &nbsp;&nbsp;&nbsp;<button id="butCx" class="btn btn-primary" type="button" style="width:100px;">查　询</button>
                        </td>
                        <td style="width: 120px;">
                            &nbsp;&nbsp;<button class="btn btn-primary" type="button" style="width:100px;" onclick="previousstep();">关　闭</button>
                        </td>
                        <td style="width: 20px;"></td>
                    </tr>
                </table>

            </td>
        </tr>
    </table>

    <table id="worktable" style="width: 1024px; background-color: white; min-height: 800px; margin-right: auto; margin-left: auto;">
        <tr>
            <td style="height:100%;vertical-align:top; " colspan="8">
                <div id="rpta">

                </div>
            </td>
        </tr>

    </table>

</body>
</html>


<script type="text/javascript" language="javascript">
    $(document).ready(function () {
        $("#butCx").click(function () {
            var s1 = $("#txtrq1").val();
            if (s1 == "" || undefined || null) {
                $("#lblMsg1").val('请输入申请日期区间。');
                return false;
            }
            var s2 = $("#txtrq2").val();
            if (s2 == "" || undefined || null) {
                $("#lblMsg1").val('请输入申请日期区间。');
                return false;
            }
            var s3 = $("#txtzddh").val();
            if (s3 == "" || undefined || null) {
                s3 = "";
            }
            var s4 = $("#txtdsr").val();
            if (s4 == "" || undefined || null) {
                s4 = "";
            }
            var s5 = $("#txtgzc").val();
            if (s5 == "" || undefined || null) {
                s5 = "";
            }
            $("#lblMsg1").val('');

            $("#rpta").load("/Report/rpt5b", { "zddh": s3, "dsr": s4, "gzc": s5, "rq1": s1, "rq2": s2 });
        });
    });


    function previousstep() {
        window.location.href = "/Home/Index";
    }
</script>



